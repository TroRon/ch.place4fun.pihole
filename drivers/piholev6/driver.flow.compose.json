{
  "actions": [
    {
      "id": "pihole_restart_dns",
      "title": {
        "en": "Restart DNS-server"
      }
    },
    {
      "id": "pihole_update_gravity",
      "title": {
        "en": "Update gravity"
      }
    },
    {
      "id": "pihole_refresh",
      "title": {
        "en": "Refresh data"
      }
    },
    {
      "id": "pihole_pause",
      "title": {
        "en": "Pause pi-hole"
      },
      "duration": true,
      "titleFormatted": {
        "en": "Pause pi-hole filtering"
      }
    }
  ],
  "triggers": [
    {
      "id": "onoff_true",
      "title": {
        "en": "Adblocking turned on"
      }
    },
    {
      "id": "onoff_false",
      "title": {
        "en": "Adblocking turned off"
      }
    },
    {
      "id": "pihole_client_query",
      "title": {
        "en": "DNS-request from client with ip ..."
      },
      "titleFormatted": {
        "en": "[[recordType]] DNS query from client with ip [[clientIp]]"
      },
      "tokens": [
        {
          "name": "queryStatus",
          "type": "string",
          "title": {
            "en": "Query status"
          },
          "placeholder": {
            "en": "DENYLIST"
          },
          "hint": {
            "en": "The status from the upstream server. 'Denylist' when the request was blocked. Forwarded, cache, ..."
          }
        },
        {
          "name": "queryDuration",
          "type": "number",
          "example": 5,
          "title": {
            "en": "Query duration in ms"
          },
          "hint": {
            "en": "Negative if not available"
          }
        },
        {
          "name": "domain",
          "type": "string",
          "placeholder": {
            "en": "example.org"
          },
          "title": {
            "en": "Domain name"
          }
        },
        {
          "name": "recordType",
          "type": "string",
          "placeholder": {
            "en": "CNAME"
          },
          "title": {
            "en": "Domain type"
          },
          "hint": {
            "en": "A/AAAA/CNAME/..."
          }
        },
        {
          "name": "clientIp",
          "type": "string",
          "placeholder": {
            "en": "127.0.0.1"
          },
          "title": {
            "en": "Client ip-address"
          }
        },
        {
          "name": "clientName",
          "type": "string",
          "placeholder": {
            "en": "computer.local"
          },
          "title": {
            "en": "Client name"
          },
          "hint": {
            "en": "As reported by Pi-Hole, not available for all clients"
          }
        }
      ],
      "args": [
        {
          "name": "clientIp",
          "type": "text",
          "title": {
            "en": "Client IP address"
          }
        },
        {
          "name": "recordType",
          "type": "dropdown",
          "title": {
            "en": "DNS Record type"
          },
          "values": [
            {
              "id": "*",
              "title": {
                "en": "Any"
              }
            },
            {
              "id": "A",
              "title": {
                "en": "A-type"
              }
            },
            {
              "id": "AAAA",
              "title": {
                "en": "AAAA-type"
              }
            },
            {
              "id": "CNAME",
              "title": {
                "en": "CNAME"
              }
            }
          ]
        }
      ]
    },
