    {
      "id": "pihole_group_add_domain",
      "title": {
        "en": "Add domain to Pi-Hole group"
      },
      "titleFormatted": {
        "en": "Add [[domain]] to Pi-Hole group [[group]]"
      },
      "hint": {
        "en": "For advanced pi-hole users only. Add domain to a Pi-Hole group, so it also applies to devices in that group. The domain should already be defined in Pi-Hole as a blocked or allowed domain. The group should be manually created in Pi-Hole first."
      },
      "args": [
        {
          "name": "domain",
          "type": "text",
          "placeholder": {
            "en": "example.org"
          }
        },
        {
          "name": "group",
          "type": "autocomplete",
          "title": {
            "en": "group name"
          }
        }
      ]
    },
    {
      "id": "pihole_group_remove_domain",
      "title": {
        "en": "Remove domain from Pi-Hole group"
      },
      "titleFormatted": {
        "en": "Remove [[domain]] to Pi-Hole group [[group]]"
      },
      "hint": {
        "en": "For advanced pi-hole users only. Remove domain from a Pi-Hole group, so it also applies to devices in that group. The domain should already be defined in Pi-Hole as a blocked or allowed domain. The domain will not be deleted, only unlinked from the group. The group should be manually created in Pi-Hole first."
      },
      "args": [
        {
          "name": "domain",
          "type": "text",
          "placeholder": {
            "en": "example.org"
          }
        },
        {
          "name": "group",
          "type": "autocomplete",
          "title": {
            "en": "group name"
          }
        }
      ]
    },
    {
      "id": "pihole_local_dns_add_record",
      "title": {
        "en": "Add local DNS record"
      },
      "titleFormatted": {
        "en": "Add local DNS record [[domain]] pointing to IP-address [[ip]]"
      },
      "hint": {
        "en": "Adds a local DNS record to Pi-Hole. This will automatically flush the DNS cache."
      },
      "args": [
        {
          "name": "domain",
          "type": "text",
          "title": {
            "en": "Domain name"
          },
          "placeholder": {
            "en": "example.org"
          }
        },
        {
          "name": "ip",
          "type": "text",
          "title": {
            "en": "Destination IP-address"
          },
          "placeholder": {
            "en": "127.0.0.1"
          }
        }
      ]
    },
    {
      "id": "pihole_local_dns_remove_record",
      "title": {
        "en": "Remove local DNS record"
      },
      "titleFormatted": {
        "en": "Remove local DNS record [[domain]]"
      },
      "hint": {
        "en": "Removes a local DNS record from Pi-Hole"
      },
      "args": [
        {
          "name": "domain",
          "type": "text",
          "title": {
            "en": "Domain name"
          },
          "placeholder": {
            "en": "example.org"
          }
        }
      ]
    }
