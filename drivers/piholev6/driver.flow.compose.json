    {
      "id": "pihole_pause",
      "highlight": true,
      "title": {
        "en": "Pause pi-hole for ... seconds"
      },
      "titleFormatted": {
        "en": "Pause pi-hole filtering for [[duration]] seconds"
      },
      "args": [
        {
          "type": "number",
          "name": "duration",
          "title": {
            "en": "Duration"
          },
          "min": 5,
          "max": 86400,
          "step": 5,
          "placeholder": {
            "en": "Seconds"
          }
        }
      ]
