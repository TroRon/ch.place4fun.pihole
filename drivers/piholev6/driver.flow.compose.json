    {
      "id": "pihole_domain_add",
      "title": {
        "en": "Add domain to list of blocked domains"
      },
      "titleFormatted": {
        "en": "Add [[domain]] to the list of [[allowedOrBlocked]] domains"
      },
      "hint": {
        "en": "Adds a domain to the list of allowed or blocked domains. The domain is added to the 'default' group in Pi-Hole"
      },
      "args": [
        {
          "name": "domain",
          "type": "text",
          "placeholder": {
            "en": "example.org"
          }
        },
        {
          "name": "allowedOrBlocked",
          "type": "dropdown",
          "title": {
            "en": "Allowed/Blocked"
          },
          "values": [
            {
              "id": "allow",
              "title": {
                "en": "allowed"
              }
            },
            {
              "id": "deny",
              "title": {
                "en": "blocked"
              }
            }
          ]
        }
      ]
    },
