    {
      "id": "pihole_client_query",
      "title": {
        "en": "DNS-request from client with ip ..."
      },
      "titleFormatted": {
        "en": "[[recordType]] DNS query from client with ip [[clientIp]]"
      },
      "tokens": [
        {
          "name": "queryStatus",
          "type": "string",
          "title": {
            "en": "Query status"
          },
          "placeholder": {
            "en": "DENYLIST"
          },
          "hint": {
            "en": "The status from the upstream server. 'Denylist' when the request was blocked. Forwarded, cache, ..."
          }
        },
        {
          "name": "queryDuration",
          "type": "number",
          "example": 5,
          "title": {
            "en": "Query duration in ms"
          },
          "hint": {
            "en": "Negative if not available"
          }
        },
        {
          "name": "domain",
          "type": "string",
          "placeholder": {
            "en": "example.org"
          },
          "title": {
            "en": "Domain name"
          }
        },
        {
          "name": "recordType",
          "type": "string",
          "placeholder": {
            "en": "CNAME"
          },
          "title": {
            "en": "Domain type"
          },
          "hint": {
            "en": "A/AAAA/CNAME/..."
          }
        },
        {
          "name": "clientIp",
          "type": "string",
          "placeholder": {
            "en": "127.0.0.1"
          },
          "title": {
            "en": "Client ip-address"
          }
        },
        {
          "name": "clientName",
          "type": "string",
          "placeholder": {
            "en": "computer.local"
          },
          "title": {
            "en": "Client name"
          },
          "hint": {
            "en": "As reported by Pi-Hole, not available for all clients"
          }
        }
      ],
      "args": [
        {
          "name": "clientIp",
          "type": "text",
          "title": {
            "en": "Client IP address"
          },
          "placeholder": {
            "en": "127.0.0.1"
          }
        },
        {
          "name": "recordType",
          "type": "dropdown",
